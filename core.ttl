#################################################################
#    Classes
#################################################################

###  http://www.beamery.com#city
beam:city rdf:type owl:Class ;
          rdfs:subClassOf beam:location .


###  http://www.beamery.com#contact
beam:contact rdf:type owl:Class ;
             rdfs:subClassOf beam:spatialEntity ;
             rdfs:comment "Link to persons subontology" .


###  http://www.beamery.com#coreSkill
beam:coreSkill rdf:type owl:Class ;
               rdfs:subClassOf beam:skill .


###  http://www.beamery.com#country
beam:country rdf:type owl:Class ;
             rdfs:subClassOf beam:location .


###  http://www.beamery.com#course
beam:course rdf:type owl:Class ;
            rdfs:subClassOf beam:skillSource .


###  http://www.beamery.com#degree
beam:degree rdf:type owl:Class .


###  http://www.beamery.com#degreeClass
beam:degreeClass rdf:type owl:Class .


###  http://www.beamery.com#education
beam:education rdf:type owl:Class ;
               rdfs:subClassOf beam:skillSource ,
                               beam:spatialEntity ,
                               beam:temporalEntity ;
               rdfs:comment "Link to education subontology" .


###  http://www.beamery.com#educationAttribute
beam:educationAttribute rdf:type owl:Class .


###  http://www.beamery.com#experience
beam:experience rdf:type owl:Class ;
                rdfs:subClassOf beam:skillSource ,
                                beam:temporalEntity .


###  http://www.beamery.com#experienceAttribute
beam:experienceAttribute rdf:type owl:Class .


###  http://www.beamery.com#language
beam:language rdf:type owl:Class .


###  http://www.beamery.com#location
beam:location rdf:type owl:Class ;
              rdfs:subClassOf beam:spatialEntity ;
              rdfs:comment "Link to existing location ontology" .


###  http://www.beamery.com#major
beam:major rdf:type owl:Class ;
           rdfs:subClassOf beam:educationAttribute .


###  http://www.beamery.com#managerVerified
beam:managerVerified rdf:type owl:Class ;
                     rdfs:subClassOf beam:verification ;
                     owl:disjointWith beam:selfVerified .


###  http://www.beamery.com#organisation
beam:organisation rdf:type owl:Class ;
                  rdfs:subClassOf beam:educationAttribute ,
                                  beam:experienceAttribute ,
                                  beam:spatialEntity ;
                  rdfs:comment "Link to organisation subontology." .


###  http://www.beamery.com#peopleSkill
beam:peopleSkill rdf:type owl:Class ;
                 rdfs:subClassOf beam:skill .


###  http://www.beamery.com#qualification
beam:qualification rdf:type owl:Class ;
                   rdfs:subClassOf beam:educationAttribute .


###  http://www.beamery.com#roleTitle
beam:roleTitle rdf:type owl:Class ;
               rdfs:subClassOf beam:experienceAttribute .


###  http://www.beamery.com#selfVerified
beam:selfVerified rdf:type owl:Class ;
                  rdfs:subClassOf beam:verification .


###  http://www.beamery.com#skill
beam:skill rdf:type owl:Class .


###  http://www.beamery.com#skillAttribute
beam:skillAttribute rdf:type owl:Class .


###  http://www.beamery.com#skillProficiency
beam:skillProficiency rdf:type owl:Class ;
                      rdfs:subClassOf beam:skillAttribute .


###  http://www.beamery.com#skillSource
beam:skillSource rdf:type owl:Class ;
                 rdfs:subClassOf beam:skillAttribute .


###  http://www.beamery.com#spatialEntity
beam:spatialEntity rdf:type owl:Class .


###  http://www.beamery.com#technicalSkill
beam:technicalSkill rdf:type owl:Class ;
                    rdfs:subClassOf beam:skill .


###  http://www.beamery.com#temporalEntity
beam:temporalEntity rdf:type owl:Class .


###  http://www.beamery.com#verification
beam:verification rdf:type owl:Class ;
                  rdfs:subClassOf beam:skillAttribute .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( beam:coreSkill
                beam:peopleSkill
                beam:technicalSkill
              )
] .


